import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        // Read two integers from input
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        
        // Print their sum
        System.out.println(a + b);
        
        scanner.close();
    }
}
