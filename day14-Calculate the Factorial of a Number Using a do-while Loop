import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        scanner.close();
        
        int result = 1;
        int i = n;
        
        if (n == 0) {
            result = 1;
        } else {
            do {
                result *= i;
                i--;
            } while (i > 0);
        }

        System.out.println(result);
    }
}
